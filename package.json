{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "author": "eggfriedrice",
  "description": "Next.js starter kit with all the bells and whistles",
  "scripts": {
    "clean": "rimraf --glob **/node_modules **/dist **/.next pnpm-lock.yaml **/.tsbuildinfo",
    "build": "next build",
    "dev": "cross-env PORT=2424 next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "check": "pnpm lint && pnpm typecheck && pnpm format:check",
    "shadcn:add": "pnpm dlx shadcn@latest add",
    "db:generate": "dotenv drizzle-kit generate",
    "db:introspect": "dotenv drizzle-kit introspect",
    "db:push": "dotenv drizzle-kit push",
    "db:migrate": "cross-env DB_MIGRATING=true dotenv tsx src/server/db/migrate.ts",
    "db:drop-migration": "drizzle-kit drop",
    "db:seed": "dotenv tsx src/server/db/seed.ts",
    "db:studio": "dotenv drizzle-kit studio"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.4",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tabler/icons-react": "^3.26.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-orm": "^0.38.2",
    "drizzle-zod": "^0.6.0",
    "jiti": "^2.4.2",
    "nanoid": "^5.0.9",
    "next": "15.1.2",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.3.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "typescript-eslint": "^8.18.1"
  }
}
